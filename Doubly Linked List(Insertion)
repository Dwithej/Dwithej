#include<stdio.h>
#include<stdlib.h>
struct node
{
    int data;
    struct node *next,*prev;
};
struct node *beg(struct node *head,int d)
{
    struct node *new=malloc(sizeof(struct node));
    new->prev=NULL;
    new->next=NULL;
    new->data=d;
    head->prev=new;
    new->next=head;
    head=new;
    return head;
}
int main()
{
    struct node *temp,*head=malloc(sizeof(struct node));
    head->data=4;
    head->prev=NULL;
    head->next=NULL;
    head=beg(head,6);
    head=beg(head,8);
    temp=head;
    while(temp!=NULL)
    {
        printf("%d\n",temp->data);
        temp=temp->next;
    }
    return 0;
}
