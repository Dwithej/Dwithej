#include<stdio.h>
int main()
{
    int a[100],i,n;
    printf("Enter size of the array(n value) :");
    scanf("%d",&n);
    printf("\nEnter array elements :");
    for(i=0;i<n;i++)
    {
    scanf("%d",&a[i]);
    }
    //priting elements in reverse order
    for(i=n-1;i>=0;i--)
    {
        printf("%d ",a[i]);
    }
    return 0;
}
